{
  "name": "kth-node-redis",
  "version": "3.0.5",
  "description": "Redis client module for Node.js. Everything with Promises!",
  "main": "lib/index.js",
  "license": "MIT",
  "author": {
    "name": "KTH",
    "email": "infosys@kth.se",
    "url": "https://github.com/KTH"
  },
  "scripts": {
    "codecheck": "prettier",
    "preversion": "npm run codecheck && npm run test",
    "postversion": "git push && git push --tags",
    "build": "./build.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "test:coverage": "jest --collectCoverage --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KTH/kth-node-redis"
  },
  "keywords": [
    "node",
    "redis"
  ],
  "peerDependencies": {
    "kth-node-log": "^3.0.2"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "kth-node-log": "^3.0.2",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-extended": "^0.11.5",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "redis-mock": "^0.49.0"
  },
  "prettier": {
    "//": "configuration version 2020-04-10 - might also reside in .prettierrc.json",
    "arrowParens": "avoid",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "//": "configuration version 2020-04-10 (API+NPM) - might also reside as module.exports in jest.config.js",
    "clearMocks": true,
    "notifyMode": "failure-change",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testEnvironment": "node",
    "verbose": true
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
