{
  "name": "kth-node-redis",
  "version": "3.1.6",
  "description": "Redis client module for Node.js. Everything with Promises!",
  "main": "lib/index.js",
  "automaticPublish": "true",
  "license": "MIT",
  "author": {
    "name": "KTH",
    "email": "infosys@kth.se",
    "url": "https://github.com/KTH"
  },
  "scripts": {
    "codecheck": "prettier",
    "preversion": "npm run codecheck && npm run test",
    "postversion": "git push && git push --tags",
    "build": "./build.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch-update": "jest --watch -u",
    "test:notify": "jest --watch --notify",
    "test:coverage": "rm -rf ./coverage && jest --collectCoverage --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KTH/kth-node-redis"
  },
  "keywords": [
    "node",
    "redis"
  ],
  "dependencies": {
    "bluebird": "^3.7.2",
    "kth-node-log": "^3.1.3",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@kth/eslint-config-kth": "^1.0.10",
    "@types/jest": "^26.0.20",
    "eslint": "^7.20.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "redis-mock": "^0.56.3"
  },
  "eslintConfig": {
    "extends": [
      "@kth/eslint-config-kth"
    ]
  },
  "prettier": {
    "//": "configuration version 2020-04-10 - might also reside in .prettierrc.json",
    "arrowParens": "avoid",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "//": "configuration version 2020-04-10 (API+NPM) - might also reside as module.exports in jest.config.js",
    "clearMocks": true,
    "notifyMode": "failure-change",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testEnvironment": "node",
    "verbose": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  }
}
